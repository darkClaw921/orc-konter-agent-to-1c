{
  "id": null,
  "uid": "sgr-agent-dashboard",
  "title": "SGR Agent - 1C Contractor Creator",
  "tags": ["sgr-agent", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Contracts Processed (Daily)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(increase(sgr_agent_contract_processing_total[24h]))",
          "legendFormat": "Contracts per day",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Contracts"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
    {
      "id": 2,
      "title": "Processing Duration (p95)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(sgr_agent_contract_processing_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 Duration",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Duration"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
    {
      "id": 3,
      "title": "LLM API Call Success Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "(sum(rate(sgr_agent_llm_api_calls_total{status=\"success\"}[5m])) / sum(rate(sgr_agent_llm_api_calls_total[5m]))) or vector(0)",
          "legendFormat": "Success Rate",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "percentunit",
          "label": "Success Rate",
          "max": 1,
          "min": 0
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
    {
      "id": 4,
      "title": "1C Integration Success Rate",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "(sum(rate(sgr_agent_1c_api_calls_total{status=\"success\"}[5m])) / sum(rate(sgr_agent_1c_api_calls_total[5m]))) or vector(0)",
          "legendFormat": "Success Rate",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "percentunit",
          "label": "Success Rate",
          "max": 1,
          "min": 0
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
    {
      "id": 5,
      "title": "Task Queue Length",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sgr_agent_queue_length",
          "legendFormat": "Queue Length",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "short",
          "label": "Tasks"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    },
    {
      "id": 6,
      "title": "API Response Time (p99)",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(sgr_agent_api_response_time_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99 Response Time",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Response Time"
        },
        {
          "format": "short"
        }
      ],
      "xaxis": {
        "mode": "time"
      }
    }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  }
}
